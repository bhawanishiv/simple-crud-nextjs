{
  "name": "simple-crud-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "cpy 'src/interfaces/**/*.swagger.yaml' public/openapi --flat",
